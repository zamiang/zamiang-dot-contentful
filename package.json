{
  "name": "zamiang-dot-contentful",
  "version": "0.0.1",
  "description": "A Node.js app using Express 4 with Webpack, React. Data stored on Contentful",
  "repository": "https://github.com/zamiang/zamiang-dot-contentful",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json server/**/* app/* app/actions/* app/components/* app/containers/* app/middlewares/* app/reducers/* app/store/*",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env NODE_ENV=production node compiled/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "yarn run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "stats": "yarn run clean && cross-env NODE_ENV=production webpack --json > stats.json",
    "postinstall": "if [ $BUILD_ASSETS ]; then yarn run build; else yarn run build:dev; fi",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "test:notify": "yarn run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify"
  },
  "author": "Brennan Moore",
  "license": "MIT",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "expect": "^1.15.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "nodemon": "^1.10.2",
    "react-addons-test-utils": "^15.4.2",
    "ts-jest": "^18.0.3",
    "tslint": "^4.4.2"
  },
  "engines": {
    "node": "6.x"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^0.0.33",
    "@types/classnames": "^0.0.32",
    "@types/express": "^4.0.35",
    "@types/graphql": "^0.8.6",
    "@types/marked": "^0.0.28",
    "@types/method-override": "^0.0.29",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "@6.0.40",
    "@types/react": "^15.0.9",
    "@types/react-dom": "^0.14.23",
    "@types/react-helmet": "^0.0.28",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^3.0.3",
    "@types/react-router-redux": "^4.0.40",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^2.6.34",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack": "^2.2.5",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "axios": "^0.15.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "body-parser": "^1.16.0",
    "classnames": "^2.2.3",
    "concurrently": "^3.3.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "graphql": "^0.9.1",
    "graphql-server": "^0.3.2",
    "graphql-server-express": "^0.6.0",
    "graphql-subscriptions": "^0.3.0",
    "graphql-tools": "^0.10.0",
    "helmet": "^3.4.0",
    "lodash.has": "^4.5.2",
    "marked": "^0.3.5",
    "method-override": "^2.3.7",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-reporter": "^3.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-markdown": "^2.4.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.8",
    "style-loader": "^0.13.1",
    "tinycolor2": "^1.4.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.7",
    "warning": "^3.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-s3-plugin": "^0.9.2"
  }
}