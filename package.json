{
  "name": "zamiang-dot-contentful",
  "version": "0.0.1",
  "description": "A Node.js app using Express 4 with Webpack, React. Data stored on Contentful",
  "repository": "https://github.com/zamiang/zamiang-dot-contentful",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json server/index.tsx",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env NODE_ENV=production node compiled/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "yarn run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "stats": "yarn run clean && cross-env NODE_ENV=production webpack --json > stats.json",
    "postinstall": "if [ $BUILD_ASSETS ]; then yarn run build; else yarn run build:dev; fi"
  },
  "author": "Brennan Moore",
  "license": "MIT",
  "devDependencies": {
    "expect": "^1.15.2",
    "nodemon": "^1.10.2",
    "null-loader": "^0.1.1",
    "react-transform-catch-errors": "^1.0.2",
    "tslint": "^4.4.2"
  },
  "engines": {
    "node": "6.x"
  },
