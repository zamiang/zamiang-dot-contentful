{
  "name": "zamiang-dot-contentful",
  "version": "0.0.1",
  "description": "A Node.js app using Express 4 with Webpack, React. Data stored on Contentful",
  "repository": "https://github.com/zamiang/zamiang-dot-contentful",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "prettier": "prettier --write \"{app,server,webpack,scripts}/**/*.{ts,tsx,js,css,graphql}\"",
    "typecheck": "tsc --project tsconfig.json --noEmit --pretty",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env NODE_ENV=production node compiled/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev":
      "cross-env NODE_ENV=development webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "build":
      "yarn run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "stats": "yarn run clean && cross-env NODE_ENV=production webpack --json > stats.json",
    "postinstall": "if [ $BUILD_ASSETS ]; then yarn run build; else yarn run build:dev; fi",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "test:notify": "yarn run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify"
  },
  "author": "Brennan Moore",
  "license": "MIT",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": ["tsx", "ts", "js", "json"]
  },
  "devDependencies": {
    "expect": "^22.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "nodemon": "^1.10.2",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.4.2",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.2"
  },
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.16.8",
    "@types/classnames": "^2.2.3",
    "@types/express": "^4.0.35",
    "@types/graphql": "^0.12.3",
    "@types/marked": "^0.3.0",
    "@types/method-override": "^0.0.31",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "8.5.7",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router": "^3.0.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/redux-logger": "^3.0.5",
    "@types/webpack": "^3.8.4",
    "autoprefixer": "^7.2.5",
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "axios": "^0.17.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "body-parser": "^1.16.0",
    "classnames": "^2.2.3",
    "concurrently": "^3.3.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "graphql": "^0.12.3",
    "graphql-server": "^0.3.2",
    "graphql-server-express": "^1.3.2",
    "graphql-tools": "^2.19.0",
    "helmet": "^3.4.0",
    "lodash.has": "^4.5.2",
    "marked": "^0.3.5",
    "method-override": "^2.3.7",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-reporter": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.1.5",
    "react-redux": "^5.0.2",
    "react-router": "3",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.5.3",
    "style-loader": "^0.20.1",
    "tinycolor2": "^1.4.1",
    "ts-loader": "^3.4.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.6.2",
    "warning": "^3.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-s3-plugin": "^1.0.0-rc.0"
  }
}
